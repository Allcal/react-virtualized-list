{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "${workspaceRoot}/index.html",
            "sourceMaps": true,
            "userDataDir": "c:\\temp\\"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost/mypage.html",
			"sourceMaps": true,
			"webRoot": "wwwroot",
            "userDataDir": "c:\\temp\\"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 9222
		}
	]
}